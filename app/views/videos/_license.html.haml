-unless video.license.blank?
  -if video.license =~ /^cc/
    %div{ "xmlns:cc" => "http://creativecommons.org/ns#" }
      This presentation, by
      =raw(display_presenters_with_external_links(video))
      , is licensed under a
      =link_to "Creative Commons Attribution-ShareAlike 3.0", video.license_url, :rel => "license"
      %br
      %a{ :rel => "license", :href => video.license_url }
        =image_tag(video.license_image_file_url)
