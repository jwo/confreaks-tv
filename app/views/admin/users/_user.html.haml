%tr
  %td
    = user.id
  %td
    = user.first_name
  %td
    = user.last_name
  %td
    = user.presenter.try(:twitter_name)
  %td
    = user.authentications.map {|a| a.provider_name}.join(", ")
  %td
    = yes_no(user.presenter)
  %td
    = yes_no(user.is_admin?)
  %td
    = user.presenter.try(:videos).try(:count)
  %td
    = user.email
  %td
    = link_to 'edit', edit_admin_user_path(user)